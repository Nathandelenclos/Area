{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api",
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "discord": {
      "type": "application",
      "root": "apps/discord",
      "entryFile": "main",
      "sourceRoot": "apps/discord/src",
      "compilerOptions": {
        "tsConfigPath": "apps/discord/tsconfig.app.json"
      }
    },
    "spotify": {
      "type": "application",
      "root": "apps/spotify",
      "entryFile": "main",
      "sourceRoot": "apps/spotify/src",
      "compilerOptions": {
        "tsConfigPath": "apps/spotify/tsconfig.app.json"
      }
    },
    "timer": {
      "type": "application",
      "root": "apps/timer",
      "entryFile": "main",
      "sourceRoot": "apps/timer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/timer/tsconfig.app.json"
      }
    },
    "service": {
      "type": "application",
      "root": "apps/service",
      "entryFile": "main",
      "sourceRoot": "apps/service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service/tsconfig.app.json"
      }
    },
    "applet": {
      "type": "application",
      "root": "apps/applet",
      "entryFile": "main",
      "sourceRoot": "apps/applet/src",
      "compilerOptions": {
        "tsConfigPath": "apps/applet/tsconfig.app.json"
      }
    },
    "news": {
      "type": "application",
      "root": "apps/news",
      "entryFile": "main",
      "sourceRoot": "apps/news/src",
      "compilerOptions": {
        "tsConfigPath": "apps/news/tsconfig.app.json"
      }
    },
    "github": {
      "type": "application",
      "root": "apps/github",
      "entryFile": "main",
      "sourceRoot": "apps/github/src",
      "compilerOptions": {
        "tsConfigPath": "apps/github/tsconfig.app.json"
      }
    },
    "slack": {
      "type": "application",
      "root": "apps/slack",
      "entryFile": "main",
      "sourceRoot": "apps/slack/src",
      "compilerOptions": {
        "tsConfigPath": "apps/slack/tsconfig.app.json"
      }
    },
    "weather": {
      "type": "application",
      "root": "apps/weather",
      "entryFile": "main",
      "sourceRoot": "apps/weather/src",
      "compilerOptions": {
        "tsConfigPath": "apps/weather/tsconfig.app.json"
      }
    }
  }
}