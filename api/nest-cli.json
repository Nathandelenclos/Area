{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api",
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "discord": {
      "type": "application",
      "root": "apps/discord",
      "entryFile": "main",
      "sourceRoot": "apps/discord/src",
      "compilerOptions": {
        "tsConfigPath": "apps/discord/tsconfig.app.json"
      }
    },
    "spotify": {
      "type": "application",
      "root": "apps/spotify",
      "entryFile": "main",
      "sourceRoot": "apps/spotify/src",
      "compilerOptions": {
        "tsConfigPath": "apps/spotify/tsconfig.app.json"
      }
    },
    "instagram": {
      "type": "application",
      "root": "apps/instagram",
      "entryFile": "main",
      "sourceRoot": "apps/instagram/src",
      "compilerOptions": {
        "tsConfigPath": "apps/instagram/tsconfig.app.json"
      }
    },
    "gmail": {
      "type": "application",
      "root": "apps/gmail",
      "entryFile": "main",
      "sourceRoot": "apps/gmail/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gmail/tsconfig.app.json"
      }
    },
    "youtube": {
      "type": "application",
      "root": "apps/youtube",
      "entryFile": "main",
      "sourceRoot": "apps/youtube/src",
      "compilerOptions": {
        "tsConfigPath": "apps/youtube/tsconfig.app.json"
      }
    },
    "google_drive": {
      "type": "application",
      "root": "apps/google_drive",
      "entryFile": "main",
      "sourceRoot": "apps/google_drive/src",
      "compilerOptions": {
        "tsConfigPath": "apps/google_drive/tsconfig.app.json"
      }
    },
    "timer": {
      "type": "application",
      "root": "apps/timer",
      "entryFile": "main",
      "sourceRoot": "apps/timer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/timer/tsconfig.app.json"
      }
    }
  }
}