{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "exclude": [
    "__tests__/**/*-test.ts"
  ],
  "typeRoots": [
    "./src/types"
  ],
  "compilerOptions": {
    "paths": {
      "@components/*": [
        "./src/Components/*"
      ],
      "@contexts/*": [
        "./src/Contexts/*"
      ],
      "@interfaces/*": [
        "./src/Interfaces/*"
      ],
      "@navigators/*": [
        "./src/Navigators/*"
      ],
      "@services/*": [
        "./src/Services/*"
      ],
      "@types/*": [
        "./src/Types/*"
      ],
      "@views/*": [
        "./src/Views/*"
      ],
      "@assets/*": [
        "./src/Assets/*"
      ],
      "@src/*": [
        "./src/*"
      ]
    }
  }
}
